{
	"name": "Sketchport",
	"description": "Exports your Sketch documents into JSON or Swift, maybe someday more.",
	"author": "Paul Jones",
	"homepage": "https://github.com/PLJNS/sketch.export-styles-and-colors/",
	"version": "0.1.0",
	"identifier": "com.pljns.sketch.sketchport",
	"compatibleVersion": "53",
	"bundleVersion": "2.0",
	"appcast": "https://github.com/PLJNS/sketch.export-styles-and-colors/master/appcast.xml",
	"commands": [
		{
			"name": "Export JSON to file...",
			"identifier": "save",
			"shortcut": "",
			"script": "save.js",
			"handler": "save"
		},
		{
			"name": "Export Swift to folder...",
			"identifier": "swift",
			"shortcut": "",
			"script": "save.js",
			"handler": "swift"
		}
	],
	"menu": {
		"items": [
			"save",
			"swift"
		]
	}
}